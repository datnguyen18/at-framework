<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Test-method Suite" parallel="tests"  thread-count="2">
    <listeners>
        <listener class-name="com.realworld.listener.TestListener"></listener>
    </listeners>
    <test name="Test-method test" parallel="methods" thread-count="2">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="com.realworld.tests.TestDemo" />
        </classes>
    </test>
</suite>
